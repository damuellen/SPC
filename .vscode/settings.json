{
    "workbench.editorAssociations": {
        "*.png": "imagePreview.previewEditor"
    },
    "terminal.integrated.defaultLocation": "editor",
    "terminal.integrated.fontSize": 16,
    "editor.fontFamily": "SF Mono, Menlo, Monaco, Consolas, monospace",
    "editor.fontSize": 16,
    "git.autofetch": true,
    "editor.suggestSelection": "first",
    "editor.quickSuggestionsDelay": 50,
    "comments.openPanel": "neverOpen",
    "debug.internalConsoleOptions": "neverOpen",
    "debug.openDebug": "neverOpen",
    "debug.onTaskErrors": "showErrors",
    "debug.showInStatusBar": "never",
    "testing.autoRun.delay": 5000,
    "debug.console.closeOnEnd": true,
    "workbench.startupEditor": "none",
    "lldb.library": "/usr/lib/liblldb.so",
    "lldb.launch.expressions": "native",
    "swiftTest.swift.logpanel": true,
    "swiftTest.swift.reloadOnTextSave": false,
}
