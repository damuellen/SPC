{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "PackageTests",
            "program": "${workspaceFolder}/.build/debug/SPCPackageTests.xctest",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "swift-build-tests"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "SolarPerformanceCalc-Debug-Print",
            "program": "${workspaceFolder}/.build/debug/SolarPerformanceCalc",
            "args": [
            //  "--excel",
                "-c", "${workspaceFolder}/COM",
            //  "-m", "${workspaceFolder}/Model.playground/Resources/AlAbdaliyah.mto"
                "-m", "${workspaceFolder}/COM/Meteofile_Hyperion1-200430_P50-TMYs_10min_wind_speed_modified.mto"
            ],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "swift-build-debug-print-SolarPerformanceCalc"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "SolarPerformanceCalc-Debug",
            "program": "${workspaceFolder}/.build/debug/SolarPerformanceCalc",
            "args": [
            //  "--excel",
                "-c", "${workspaceFolder}/COM",
            //  "-m", "${workspaceFolder}/Model.playground/Resources/AlAbdaliyah.mto"
                "-m", "${workspaceFolder}/COM/Midelt.mto"
            ],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "swift-build-debug-SolarPerformanceCalc"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "SolarPerformanceCalc-Release",
            "program": "${workspaceFolder}/.build/release/SolarPerformanceCalc",
            "args": [
                "-o", "12",
                "-c", "${workspaceFolder}/COM",
                "-m", "${workspaceFolder}/COM/Meteofile_Hyperion1-200430_P50-TMYs_10min_wind_speed_modified.mto"
            ],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "swift-build-release"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "SolarFieldCalc",
            "program": "${workspaceFolder}/.build/debug/SolarFieldCalc",
            "args": [
                "--loops", "212",
                "--massflow", "606"
            ],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "swift-build-debug"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "PinchPointTool",
            "program": "${workspaceFolder}/.build/debug/PinchPointTool",
            "args": [
                "-json", "-pdf", "-html"
            ],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "swift-build-debug"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "TransTES",
            "program": "${workspaceFolder}/.build/debug/TransTES",
            "args": ["</root/workspace/SPC/kjvbible_x10.txt"],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "swift-build-debug"
        }
    ]
}
