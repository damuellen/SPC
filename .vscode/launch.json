    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug SPC",
            "program": "${workspaceFolder}/.build/debug/SPC",
            "args": [
                //  "--excel",
                "-c",
                "${workspaceFolder}/COM",
                //  "-m", "${workspaceFolder}/Model.playground/Resources/AlAbdaliyah.mto"
                "-m",
                "${workspaceFolder}/COM/Meteofile_Hyperion1-200430_P50-TMYs_10min_wind_speed_modified.mto"
            ],
            "cwd": "${workspaceFolder:SPC}",
            "preLaunchTask": "swift-build-debug-SPC"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug SolarFieldCalc",
            "program": "${workspaceFolder}/.build/debug/SolarFieldCalc",
            "args": [
                "--loops",
                "212",
                "--massflow",
                "606"
            ],
            "cwd": "${workspaceFolder:SPC}",
            "preLaunchTask": "swift-build-debug-SolarFieldCalc"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug TransTES",
            "program": "${workspaceFolder}/.build/debug/TransTES",
            "args": [],
            "cwd": "${workspaceFolder:SPC}",
            "preLaunchTask": "swift-build-debug-TransTES"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug PinchPointTool",
            "program": "${workspaceFolder}/.build/debug/PinchPointTool",
            "args": [
                "-json",
                "-pdf",
                "-html"
            ],
            "cwd": "${workspaceFolder:SPC}",
            "preLaunchTask": "swift-build-debug-PinchPointTool"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug SunOl",
            "program": "${workspaceFolder}/.build/debug/SunOl",
            "args": [],
            "cwd": "${workspaceFolder:SPC}",
            "preLaunchTask": "swift-build-debug-SunOl"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug Playground",
            "program": "${workspaceFolder}/.build/debug/Playground",
            "args": [],
            "cwd": "${workspaceFolder:SPC}",
            "preLaunchTask": "swift-build-debug-Playground"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Test SPC",
            "program": "./.build/debug/SPCPackageTests.xctest",
